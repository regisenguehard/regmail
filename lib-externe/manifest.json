   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Email Extractor",
   "offline_enabled": true,
   "permissions": [ "storage", "activeTab" ],
   "short_name": "Email Extractor",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.4.2"
}
